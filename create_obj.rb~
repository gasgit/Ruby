!#/usr/bin/ruby
print "Create Objects Ruby \n"


hello = Object.new
def hello.talk
    puts"Hello from format 1"
end

def hello.laugh
    puts "Ha ha from format 1"
end


hello.talk
puts "#{hello.laugh}


class Hello
    def talk
        puts"I am a talking object blah blah!!"
        puts("Do u object!")
    end
    def laugh
        puts"HA HA HA"
    end
end

obj = Hello.new
obj.talk
obj.laugh
